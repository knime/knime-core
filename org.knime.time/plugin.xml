      </dataValue>
      <dataValue
            valueClass="org.knime.core.data.time.zoneddatetime.ZonedDateTimeValue">
         <renderer
               rendererFactoryClass="org.knime.core.data.time.zoneddatetime.ZonedDateTimeValueRenderer$DefaultRendererFactory"
               suggestAsDefault="true">
         </renderer>
      </dataValue>
      <dataValue
            valueClass="org.knime.core.data.time.duration.DurationValue">
         <renderer
               rendererFactoryClass="org.knime.core.data.time.duration.DurationISOValueRenderer$DefaultRendererFactory"
               suggestAsDefault="false">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.core.data.time.duration.DurationLongValueRenderer$DurationLongRendererFactory"
               suggestAsDefault="false">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.core.data.time.duration.DurationShortValueRenderer$DurationShortRendererFactory"
               suggestAsDefault="true">
         </renderer>
      </dataValue>
      <dataValue
            valueClass="org.knime.core.data.time.period.PeriodValue">
         <renderer
               rendererFactoryClass="org.knime.core.data.time.period.PeriodISOValueRenderer$DefaultRendererFactory"
               suggestAsDefault="false">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.core.data.time.period.PeriodShortValueRenderer$PeriodShortRendererFactory"
               suggestAsDefault="true">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.core.data.time.period.PeriodLongValueRenderer$PeriodLongRendererFactory"
               suggestAsDefault="false">
         </renderer>
      </dataValue>
   </extension>
   <extension
         point="org.knime.core.DataType">
      <DataType
            cellClass="org.knime.core.data.time.localtime.LocalTimeCell"
            factoryClass="org.knime.core.data.time.localtime.LocalTimeCellFactory">
