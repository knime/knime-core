      </xs:attribute>

      <xs:attribute name="compareFunction" type="COMPARE-FUNCTION" use="optional" />
      <xs:attribute name="minimum" type="NUMBER" use="optional"/>
      <xs:attribute name="maximum" type="NUMBER" use="optional"/>
